{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "features": {
    "ghcr.io/devcontainers/features/java:1": {
      "version": "17",
      "installMaven": "true",
      "mavenVersion": "latest"
    },
    "ghcr.io/devcontainers-extra/features/apt-packages:1": {
			"packages": [
				"git-lfs"
			]
		}
  },
  "customizations": {
		"vscode": {
			"extensions": [
				"vscjava.vscode-java-pack"
			]
		}
	}
}
